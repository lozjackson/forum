{{#uic-button-group}}
  {{uic-button text="Cancel" action=(action cancel)}}
  {{uic-button text="Save" action=(action submit) disabled=(if changeset.isInvalid true) class="blue"}}
{{/uic-button-group}}

{{uic-form-element
  label="Password"
  value=changeset.password
  type="password"}}
{{uic-form-element
  label="Password confirmation"
  value=changeset.passwordConfirmation
  type="password"}}
